extends layout

block corpo
    .w3-container
        .w3-card-4
            header.w3-container.w3-light-gray
                h1 Obras Musicais
            .w3-container
                table.w3-table.w3-bordered
                    tr
                        th id
                        th titulo 
                    each obras in ind
                        tr
                            td=obras.id
                            td
                                a(href='http://localhost:5000/titulo/' + obras.id)= obras.titulo
            footer.w3-container.w3-light-gray
                adress Gerado por PubsApp